!!!
%html
  %head
    %title TigerPride
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular.min.js', 'data-turbolinks-track': 'reload'
    = yield :style_include
    = yield :script_include
    = yield :style_markup
    = yield :script_markup
  %body
    = yield